{
  "name": "sssync",
  "version": "0.4.0",
  "description": "ePlus Smart Sale Sync - Sync data from local pc to web and vice verse",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ sssync --overwrite --asar --platform win32 --icon=assets/icons/win/icon.ico --prune=true --out=builds --version-string.CompanyName=\"Milestone Innovative Technologies\" --version-string.FileDescription=\"ePlus Smart Sale Database Synchronizer\" --version-string.ProductName=\"ePlus Smart Sale Synchronizer\"",
    "setup": "node setup/setup.js",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "build": {
    "appId": "sssync",
    "productName": "ePlus Smart Sale Synchronizer"
  },
  "win": {
    "icon": "assets/icons/win/icon.ico",
    "target": "NSIS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilestoneInnovativeTechnologies/ePlusSmartSaleSynchronizer.git"
  },
  "keywords": [
    "ePlus",
    "Smart",
    "Sale",
    "Milestone",
    "IT",
    "Syncronizer"
  ],
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "author": "Milestone Innovative Technologies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MilestoneInnovativeTechnologies/ePlusSmartSaleSynchronizer/issues"
  },
  "homepage": "https://github.com/MilestoneInnovativeTechnologies/ePlusSmartSaleSynchronizer#readme",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "jquery": "^3.4.0",
    "lodash": "^4.17.13",
    "mysql": "^2.16.0",
    "uuid": "^3.3.2",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^20.44.4",
    "electron-installer-windows": "^1.1.1",
    "electron-packager": "^13.1.1",
    "@electron-forge/cli": "^6.0.0-beta.39",
    "@electron-forge/maker-deb": "^6.0.0-beta.39",
    "@electron-forge/maker-rpm": "^6.0.0-beta.39",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.39",
    "@electron-forge/maker-zip": "^6.0.0-beta.39"
  }
}
